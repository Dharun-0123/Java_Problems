package zoho_train;

import java.lang.reflect.Array;
import java.util.Arrays;

public class Bubble_sort_arrya {

	public static void main(String[] args) {
int arr[]= {1,235,22,5236,12,523,623,6355,2,4,5};
for(int i=0;i<arr.length-1;i++) {
	for(int j=0;j<arr.length-1-i;j++){
		if(arr[j]<arr[j+1]) {
		int temp=arr[j];
		arr[j]=arr[j+1];
		arr[j+1]=temp;
		}
		}
		
}


	for(int ar:arr) {
		System.out.print(ar+" ");
	}
	

	}

}
